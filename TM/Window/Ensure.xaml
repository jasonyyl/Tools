<Window x:Class="TM.Ensure"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TM"
        mc:Ignorable="d"
        Title="提示" Height="200" Width="400" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="bt_ensure" Content="确定" HorizontalAlignment="Left" Margin="102,120,0,0" VerticalAlignment="Top" Width="75" Click="bt_ensure_Click"/>
        <Button x:Name="bt_cancle" Content="取消" HorizontalAlignment="Left" Margin="222,120,0,0" VerticalAlignment="Top" Width="75" Click="bt_cancle_Click"/>
        <TextBlock x:Name="tb_title" HorizontalAlignment="Center" Margin="0,50,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>

    </Grid>
</Window>
